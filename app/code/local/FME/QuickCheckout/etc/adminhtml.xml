<config>
    <acl>
        <resources>
            <all>
                <title>Allow Everything</title>
            </all>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <quickcheckout translate="title">
                                        <title>QuickCheckout Module</title>
                                        <sort_order>10</sort_order>
                                    </quickcheckout>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>
</config>